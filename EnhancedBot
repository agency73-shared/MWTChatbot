<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00DU9000005p1gj',
				'New_Chat_Deployment',
				'https://midwesttape--partial.sandbox.my.site.com/ESWNewChatDeployment1757266411355',
				{
					scrt2URL: 'https://midwesttape--partial.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://midwesttape--partial.sandbox.my.site.com/ESWNewChatDeployment1757266411355/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>
